int main() {
    int x := 10, y := 20;
    
    // Simple if
    if (x < y) {
        x := x + 1;
    }
    
    // If-else
    if (x > y) {
        x := x * 2;
    } else {
        y := y / 2;
    }
    
    // Nested if
    if (x == 10) {
        if (y == 20) {
            x := 0;
        }
    }
    
    return 0;
}