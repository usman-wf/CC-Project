int main() {
    int i := 0, sum := 0;
    
    // Simple while
    while (i < 10) {
        sum := sum + i;
        i := i + 1;
    }
    
    // Nested while
    i := 0;
    while (i < 3) {
        int j := 0;
        while (j < 3) {
            sum := sum + 1;
            j := j + 1;
        }
        i := i + 1;
    }
    
    return sum;
}